{
  "name": "resume-api",
  "version": "0.0.1",
  "description": "backend api for resume items",
  "author": "Jeff Rossi <rossi.jeff@gmail.com>",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prepare": "npm run build",
    "pretest": "npm run build",
    "build": "tsc -p tsconfig.build.json",
    "deploy": "gcloud app deploy --project resume-api-274820",
    "start": "NODE_ENV=production node main.js",
    "start:dev": "nodemon",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm",
    "run-migration": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:run",
    "revert-migration": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:revert",
    "schema-sync": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm schema:sync",
    "create-migration": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:create -n"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/graphql": "^7.1.5",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/typeorm": "^7.0.0",
    "@slack/web-api": "^5.8.1",
    "apollo-server-express": "^2.11.0",
    "axios": "^0.19.2",
    "bcrypt": "^4.0.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.1",
    "config": "^3.3.1",
    "crypto": "^1.0.1",
    "graphql": "^14.6.0",
    "graphql-subscriptions": "^1.1.0",
    "graphql-tools": "^4.0.7",
    "mysql": "^2.18.1",
    "node-fetch": "^2.6.7",
    "nodemon": "^2.0.3",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "shortid": "^2.2.15",
    "tsscmp": "^1.0.6",
    "typeorm": "^0.2.24",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/express": "^4.17.3",
    "@types/jest": "25.1.4",
    "@types/node": "^13.9.1",
    "@types/passport-jwt": "^3.0.3",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^25.1.0",
    "lodash": "^4.17.15",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "25.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  }
}
